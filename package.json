{
  "name": "to-tokio",
  "version": "3.8.0",
  "private": true,
  "description": "Web about Tokio. The web is based on UOC boilerplate",
  "scripts": {
    "parcel:serve": "parcel src/index.html -p 8123 --target web --open",
    "parcel:build": "parcel build src/index.html --target web --no-source-maps --no-cache",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean parcel:serve",
    "build": "npm-run-all clean parcel:build",
    "stylelint": "stylelint src/**/*.scss",
    "test": "echo 'Everything is working as expected âœ…\nStart working on your project by running \\033[1mnpm run dev\\033[0m'"
  },
  "web": "dist/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmartinezhernandez/to-tokio"
  },
  "author": {
    "name": "Jesus Martinez",
    "email": "jmartinezhernandez1@uoc.edu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmartinezhernandez/to-tokio"
  },
  "homepage": "https://github.com/jmartinezhernandez/to-tokio#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.11.0",
    "autoprefixer": "^10.4.15",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.11.0",
    "postcss-preset-env": "^9.1.1",
    "posthtml-include": "^1.7.4",
    "rimraf": "^5.0.1",
    "sharp": "^0.31.1",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1"
  }
}
